<Window x:Class="Spacebridge.KeyManager"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Spacebridge"
        mc:Ignorable="d"
        Title="Key Manager" Height="400" Width="500">
    <Grid Background="#4e6cff">
        <Label Content="Manage your Spacebridge Keys:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Foreground="White" FontWeight="Bold" FontSize="18" Width="287"/>
        <ScrollViewer Margin="0,60,0,0" Background="#253054">
            <Grid Name="KeyScrollGrid" Margin="0,0,0,0"/>
        </ScrollViewer>

    </Grid>
</Window>
